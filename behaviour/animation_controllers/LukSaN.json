{
	"format_version" : "1.10.0",
	"animation_controllers": {
		"controller.animation.luksan": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [ "default" ],
					"transitions": [
						{ "gliding": "query.is_gliding" },
						{ "sneaking": "query.is_sneaking" },
						{ "lukmoving": "query.is_luk" },
						{ "luk_correr": "query.is_sprinting" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"gliding": {
					"animations": [ "gliding" ],
					"transitions": [
						{ "lukmoving": "query.is_luk" },
						{ "luk_correr": "query.is_sprinting" },
						{ "default": "!query.is_gliding" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"sneaking": {
					"animations": [ "sneaking" ],
					"transitions": [
						{ "gliding": "query.is_gliding" },
						{ "lukmoving": "query.is_moving" },
						{ "luk_correr": "query.is_sprinting" },
						{ "default": "!query.is_sneaking" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"lukmoving": {
					"animations": [ "lukmoving" ],
					"transitions": [
						{ "gliding": "query.is_gliding" },
						{ "sneaking": "query.is_sneaking" },
						{ "luk_correr": "query.is_sprinting" },
						{ "default": "!query.is_luk" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				},
				"luk_correr": {
					"animations": [ "luk_correr" ],
					"transitions": [
						{ "gliding": "query.is_gliding" },
						{ "sneaking": "query.is_sneaking" },
						{ "lukmoving": "query.is_luk" },
						{ "default": "!query.is_sprinting" }
					],
					"blend_transition": 0.1,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}
